  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.4",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.1.3",
    "prisma": "^5.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
